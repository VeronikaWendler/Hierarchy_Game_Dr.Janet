/ontrialbegin = [
    values.POS = list.MoneyPos.nextvalue;
    values.NEG = list.MoneyNeg.nextvalue;
    values.Side = list.Side.nextvalue;
    
    if (values.Side == 1) {
        values.trialLEFT = values.POS;
        values.trialRIGHT = values.NEG;
    } else if (values.Side == 2) {
        values.trialLEFT = values.NEG;
        values.trialRIGHT = values.POS;
    }
